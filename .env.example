# SwasthyaFlow AI Backend Environment Variables

# Application
APP_NAME=SwasthyaFlow AI
VERSION=1.0.0
DEBUG=false

# Database
DATABASE_URL=postgresql+asyncpg://postgres:postgres@localhost:5432/swasthyaflow

# Redis
REDIS_URL=redis://localhost:6379/0

# MinIO/S3 Storage
MINIO_ENDPOINT=localhost:9000
MINIO_ACCESS_KEY=minioadmin
MINIO_SECRET_KEY=minioadmin
MINIO_BUCKET=ehr-documents
MINIO_SECURE=false

# JWT Authentication
SECRET_KEY=your-super-secret-key-change-in-production
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30

# ML Model Paths
MODEL_PATH=data/models
TRIAGE_MODEL_PATH=data/models/triage_model.pkl
SYMPTOM_ENCODER_PATH=data/models/symptom_encoder.pkl

# Thresholds
HIGH_LOAD_THRESHOLD=0.8
CRITICAL_LOAD_THRESHOLD=0.95
OUTBREAK_CLUSTER_MIN_SIZE=5
OUTBREAK_TIME_WINDOW_HOURS=72

